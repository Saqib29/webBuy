
@{
    Layout = "~/Views/Shared/_sellerLayout.cshtml";
}

@if (TempData["update-msg"] != null)
{
    <div class="alert alert-success small" role="alert">
        @TempData["update-msg"]
    </div>
}
else if (TempData["msg"] != null)
{
    <div class="alert alert-danger small" role="alert">
        @TempData["msg"]
    </div>
}
else
{
    <br />
}


<h1>Seller</h1>
